{
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "decorators": false,
      "dynamicImport": true
    },
    "target": "es2022",
    "loose": false,
    "externalHelpers": false,
    "keepClassNames": false,
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@/graphql/*": ["src/graphql/*"],
      "@/utils/*": ["src/utils/*"],
      "@/utils/gov/*": ["src/utils/gov/*"],
      "@/utils/nara/*": ["src/utils/nara/*"],
      "@/execs/*": ["src/execs/*"],
      "@/types/*": ["src/types/*"],
      "@/database/*": ["src/database/*"]
    }
  },
  "module": {
    "type": "es6",
    "strict": false,
    "strictMode": true,
    "lazy": false,
    "noInterop": false
  },
  "minify": false,
  "isModule": true,
  "sourceMaps": true
}