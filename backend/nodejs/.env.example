# Environment variables for Node.js backend

# Server Configuration
NODE_ENV=development
PORT=4000

# Database Configuration
DB_HOST=localhost
DB_PORT=3306
DB_USER=your_username
DB_PASSWORD=your_password
DB_NAME=your_database

# Data.go.kr API Configuration (나라장터)
# Get your service key from: https://www.data.go.kr/
DATA_GO_KR_SERVICE_KEY=your_data_go_kr_service_key_here

# Browser Configuration (for scraping)
HEADLESS=true
CHROMIUM_EXECUTABLE_PATH=

# Logging
LOG_LEVEL=info